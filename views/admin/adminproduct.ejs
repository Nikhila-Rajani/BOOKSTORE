        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Products</h2>
                    <!-- <p>Lorem ipsum dolor sit amet.</p> -->
                </div>
                <div>
                    <!-- <a href="#" class="btn btn-light rounded font-md">Export</a>
                    <a href="#" class="btn btn-light rounded  font-md">Import</a> -->
                    <a href="addProduct" class="btn btn-primary btn-sm rounded">Add New Product</a>
                </div>
            </div>
            <div class="card mb-4">
                <header class="card-header">
                    <div class="row gx-3">
                        <div class="col-lg-4 col-md-6 me-auto">
                            <input type="text" placeholder="Search..." class="" style="width: 200px; height: 30px;margin-right: 100px;">
                        </div>
                        <div class="col-lg-2 col-6 col-md-3">
                            <select class="" name="catName" style="width: 150px; height: 30px;margin-right: 100px;" aria-placeholder="Catagory">
                              
                            </select>
                        </div>
                        <div class="table-responsive">
                            <table class="table table-bordered table-sm">
                                <thead>

                                    <tr>
                                        <th class="text-center">Image</th>
                                        <th class="text-center">Name</th>
                                        <th class="text-center">Regular price</th>
                                        <th class="text-center">Offer Price</th>
                                        <th class="text-center">Stock</th>
                                        <th class="text-center">Action</th>
                                    </tr>
                                </thead>
                                <tbody>
                                  <% Product.forEach(element => { %>
           
                                   
                                 
                                        <tr>
                                            <td class="align-middle text-center" style="width: 100px;">
                                                <img style="width: 100%;" src="/uploads/<%= element.image[0] %>" alt="Product">
                                            </td>
                                            <td class="align-middle text-center">
                                                <a href="#" class="text-truncate"><%= element.name %></a>
                                            </td>
                                            <td class="align-middle text-center">
                                               <%= element.regularPrice %>
                                            </td>
                                            <td class="align-middle text-center">
                                                <%= element.offerPrice %>
                                            </td>
                                            

                                           
                                            <td class="align-middle text-center"> <%= element.stock %> </td>
                                            <td class="align-middle text-center">
                                                <% if (element.is_blocked === true) { %>
                                                    <a href="/admin/productUnblock?_id=<%= element._id %>" class="btn btn-sm font-sm btn-light rounded unlist-btn" style="background-color: green;">
                                                       unblock
                                                    </a>
                                                <% } else { %>
                                                    <a href="/admin/productBlock?_id=<%= element._id %>" class="btn btn-sm font-sm btn-light rounded unlist-btn" style="background-color: red;">
                                                       Block
                                                    </a>
                                                   
                                               <%  } %>
                                                       
                                                
                                                        <a href="/admin/editProduct?_id=<%= element._id %>" class="btn btn-sm font-sm rounded btn-brand">
                                                            <i class="material-icons md-edit"></i> Edit
                                                        </a>
                                                        
                                                    
                                                </td>
                                           
                                        </tr>
                                        <% }) %>
                                    
                                </tbody>
                            </table>
                        </div>
                        
                        
                        
                        
